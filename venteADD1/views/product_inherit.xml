<odoo>
<data>

<record id="product_form_inheritFirst" model="ir.ui.view">
        <field name="name">product_form_inheritFirst</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//form/sheet/div[3]/span[2]" position="after">
               <span class="d-inline-block">
                            <field name="parc_ok"/>
                            <label for="parc_ok"/>
               </span>
            </xpath>

        </field>
    </record>
  <record id="product_form_inheritFirst1" model="ir.ui.view">
        <field name="name">product_form_inheritFirst1</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='barcode']" position="after">
                <field name="serie_number_materiel" />
            </xpath>
        </field>
    </record>


</data>
</odoo>