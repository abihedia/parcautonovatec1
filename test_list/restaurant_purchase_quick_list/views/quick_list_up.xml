<odoo>
    <data>

  <record model="ir.ui.view" id="productquicktemplate_list_view">
      <field name="name">productquicktemplate list</field>
      <field name="model">productquicktemplate</field>
      <field name="arch" type="xml">
        <tree editable ="bottom">
          <field name="quick_product_id"/>
            <button name="action_remove" string="-" type="object" class ="oe_highlight" />
          <field name="quick_qte_product"/>
            <button name="action_add" string="+" type="object" class ="oe_highlight" />
          <field name="quick_vendor_num"/>
          <field name="quick_price_purchase"/>
        </tree>
      </field>
    </record>

<record model="ir.ui.view" id="Quik_List_up_form_view">
      <field name="name">Quik List</field>
      <field name="model">quicklast</field>
      <field name="arch" type="xml">
        <form>
            <header>

                        <button name="send_po" type="object" string="Send.P.O" icon="fa-external-link" class="oe_right oe_button_box " />
                        <button name="refrech_po" type="object" string="Refresh The List" icon="fa-check" class="oe_right oe_button_box" />
            </header>
            <sheet>
                 <div class="oe_button_box" name="button_box">
                            <button name="action_open_rfq" type="object" class="oe_stat_button" icon="fa-credit-card">
                                <div class="o_stat_info">
                                    <field name="purcahse_order_count" class="o_stat_value"/>
                                    <span class="o_stat_text"> P.O</span>
                                </div>
                            </button>
                 </div>
                <b> <field name="name" /> </b>
                <field name="qck_ProductQuick_id"  widget="one2many"/>
            </sheet>
       </form>
      </field>
</record>
<record model="ir.ui.view" id="Quik_List_up_tree_view">
      <field name="name">Quik Tree</field>
      <field name="model">quicklast</field>
      <field name="arch" type="xml">
        <tree>
             <field name="name" />
       </tree>
      </field>
</record>

<record model="ir.actions.act_window" id="Quick_Order_windowlast">
      <field name="name">Quick Order list</field>
      <field name="res_model">quicklast</field>
      <field name="view_mode">tree,form</field>
</record>


<menuitem name="Quick Order list"
          id="quick_order_res_list_2"
          parent="purchase.menu_procurement_management"
          action="Quick_Order_windowlast"
          sequence="1002"
/>


</data>
</odoo>